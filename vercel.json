{
    "version": 2,
    "builds": [
        { 
            "src": "*.js", 
            "use": "@vercel/node" 
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}
